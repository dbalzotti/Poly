    <!-- The search by index field title in online-help output -->
    <variable id="Search index field title">搜尋關鍵字：</variable>

    <!-- The search by index button title in online-help output -->
    <variable id="Search index button title">檢視</variable>

    <!-- The search by text field title in online-help output -->
    <variable id="Search text field title">搜尋說明中的項目：</variable>

    <!-- The search by text button title in online-help output -->
    <variable id="Search text button title">搜尋</variable>

	<variable id="Online Help Search Method Field">搜尋方法</variable>
	<variable id="Online Help Search Method Or">或</variable>
	<variable id="Online Help Search Method And">且</variable>
	<variable id="Search Highlight Switch">在搜尋結果中反白顯示文字</variable>
	<variable id="Search index next button title">顯示下一個</variable>
	<variable id="Search Whole Words Switch">全字拼寫須相符</variable>
	<variable id="Search Case Sensitive Switch">大小寫須相符</variable>
	<variable id="Search Stopped Message">找到 50 筆相符項目後即停止搜尋。</variable>
