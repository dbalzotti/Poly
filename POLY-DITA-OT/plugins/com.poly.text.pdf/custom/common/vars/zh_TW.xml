	<variable id="Search Search in Progress Message">搜尋中...</variable>
	<variable id="Search Search Give No Results Message">找不到符合項目。</variable>

    <!--Label for a step with importance="optional"-->
    <variable id="Optional Step">選用：</variable>

    <!--Labels for task sections-->
    <variable id="Task Prereq">開始前</variable>
    <variable id="Task Context">關於此工作</variable>
    <variable id="Task Steps">程序</variable>
    <variable id="Task Result">結果</variable>
    <variable id="Task Example">範例</variable>
    <variable id="Task Postreq">下一步</variable>
     
     <variable id="confidential">CONFIDENTIAL</variable>
