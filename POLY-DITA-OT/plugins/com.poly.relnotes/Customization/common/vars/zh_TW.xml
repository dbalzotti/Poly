    <!-- Text to use for part titles.-->
    <variable id="Part with number">第 <param ref-name="number"/> 部份</variable>

    <!-- Text to use for a label generated from <required-cleanup> element. -->
    <variable id="Required-Cleanup">[需要清除]</variable>

    <!-- Template for generated page number of a reference 
         in printed manual. -->
    <variable id="On the page"> 在第 <param ref-name="pagenum"/> 頁</variable>

    <!-- Template for generated page number of a reference
         in printed manual, when referenced element has no title. -->
    <variable id="Page"> 第 <param ref-name="pagenum"/> 頁</variable>

    <!-- Template for generated page number of a reference
         in chm/html, when referenced element has no title. -->
    <variable id="This link">此連結</variable>
     
