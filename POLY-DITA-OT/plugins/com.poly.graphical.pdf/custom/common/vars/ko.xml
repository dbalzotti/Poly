    <!-- The string used to label an notices in body text. -->
    <variable id="Notices title">고지 사항</variable>

    <!-- The heading to put at the top of a chapter when creating a
         chapter-level "mini-TOC" -->
    <variable id="Mini Toc">항목: </variable>

    <!-- Text to use for 'Note' label generated from <note> element. -->
    <variable id="Note">참고</variable>

    <!-- Text to use for 'Note' label generated from <note> element 
         with 'type' attribute set to 'tip'. -->
    <variable id="Tip">팁</variable>

    <!-- Text to use for 'Note' label generated from <note> element
         with 'type' attribute set to 'tip'. -->
    <variable id="Important">중요</variable>

    <!-- Text to use for 'Note' label generated from <note> element 
         with 'type' attribute set to 'fastpath'. -->
    <variable id="Fastpath">빠른 경로</variable>

    <!-- Text to use for 'Note' label generated from <note> element 
         with 'type' attribute set to 'remember'. -->
    <variable id="Remember">기억</variable>

    <!-- Text to use for 'Note' label generated from <note> element 
         with 'type' attribute set to 'attention'. -->
    <variable id="Attention">주의</variable>

    <!-- Text to use for 'Note' label generated from <note> element 
         with 'type' attribute set to 'caution'. -->
    <variable id="Caution">주의</variable>

    <!-- Text to use for 'Note' label generated from <note> element 
         with 'type' attribute set to 'danger'. -->
    <variable id="Danger">위험</variable>

    <!-- Text to use for 'Note' label generated from <note> element
         with 'type' attribute set to 'restriction'. -->
    <variable id="Restriction">제한</variable>

    <!-- Text to use for figure titles. Renders as part of <fig>/<title>
          elements. -->
    <variable id="Figure">그림 <param ref-name="number"/>: <param ref-name="title"/></variable>

    <!-- Text to use for table titles. Renders as part of <table>/<title> 
         elements. -->
    <variable id="Table">표 <param ref-name="number"/>: <param ref-name="title"/></variable>

    <!-- Text to use for related links label.
         Renders as part of <related-links> element. -->
    <variable id="Related Links">관련 링크</variable>

    <!-- Default cross-reference text prefix. Used to generate reference 
         text in case it's not specified in source. -->
    <variable id="Cross-Reference">상호 참조:</variable>

    <!-- Default content-reference text prefix. Used to generate reference 
         text in case target cannot be resolved. -->
    <variable id="Content-Reference">콘텐츠 참조:</variable>

    <!-- Default title text for referenced section title. Used to generate 
         reference text in case reference has no explicit content and
         referenced section has no title. -->
    <variable id="Untitled section">제목 없는 섹션.</variable>

    <!-- Default title text for referenced list item. Used to generate
         reference text in case reference has no explicit content and 
         reference target is a list item. -->
    <variable id="List item">목록 항목.</variable>

    <!-- Default title text for referenced footnote. Used to generate 
         reference text in case reference has no explicit content and 
         reference target is a footnote. -->
    <variable id="Foot note">각주.</variable>

    <!-- Text to use for a generated label of navigation title. Renders
         as part of <navtitle> element. -->
    <variable id="Navigation title">탐색 제목</variable>

    <!-- Text to use for a generated label of search title. Renders 
         as part of <searchtitle> element. -->
    <variable id="Search title">제목 검색</variable>

    <!-- Text to use for chapter titles.-->
    <variable id="Chapter with number">챕터 <param ref-name="number"/></variable>

    <!-- Text to use for appendix titles.-->
    <variable id="Appendix with number">부록 <param ref-name="number"/></variable>

    <!-- Text to use for part titles.-->
    <variable id="Part with number">파트 <param ref-name="number"/></variable>

    <!-- Text to use for a label generated from <required-cleanup> element. -->
    <variable id="Required-Cleanup">[정리 필요]</variable>

    <!-- Template for generated page number of a reference 
         in printed manual. -->
    <variable id="On the page"> <param ref-name="pagenum"/>페이지의</variable>

    <!-- Template for generated page number of a reference
         in printed manual, when referenced element has no title. -->
    <variable id="Page"> <param ref-name="pagenum"/>페이지</variable>

    <!-- Template for generated page number of a reference
         in chm/html, when referenced element has no title. -->
    <variable id="This link">이 링크</variable>

